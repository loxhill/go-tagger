[
    {
        "field": "Email",
        "rules": [
            {
                "type": "contains",
                "value": "@loxhill.com",
                "tag": "loxhill"
            }
        ]
    },
    {
        "field": "Items",
        "rules": [
            {
                "type": "count",
                "op": "gteq",
                "value": 2,
                "tag": "multiple-items"
            }
        ]
    },
    {
        "field": "Items.Title",
        "rules": [
            {
                "type": "contains",
                "value": "jumper",
                "tag": "clothing"
            }
        ]
    },
    {
        "field": "Items.Options.Weight",
        "rules": [
            {
                "type": "count",
                "op": "gt",
                "value": 0.3,
                "tag": "heavy"
            }
        ]
    }
]